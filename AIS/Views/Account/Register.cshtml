@using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @id = "RegisterUser", role = "form", enctype = "multipart/form-data" }))
{
    <div class="container-fluid">
        <div class="container">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 pricing-hd register-sec setFromTop ">
                <h1>Get Started with the <strong>Mondo<span>fi</span></strong> Venue Management System</h1>
                <p>We love hearing from people about their unique businesses. Fill out the form below or send an email to <br><a href="mailto:accounts@mondofi.com">accounts@mondofi.com</a> and tell us a little bit about you and your venue.</p>

            </div>

            @Html.AntiForgeryToken()

            @Html.Hidden("Address.City", "test")
            @Html.Hidden("Address.State", "test")
            @Html.Hidden("Address.PostalCode", "test")
            @Html.Hidden("Address.CountryId", "2")
            @Html.Hidden("price", price.ToString());
            @Html.ValidationSummary(true)

            <div class="col-xs-12  col-sm-12 col-md-12 reg-form">
                <div class="col-lg-12 center-block noLeft clearfix">
                    <div class="col-lg-12 col-sm-12 clearfix">
                        <div class="row">

                            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 form-group">

                                <div class="cust-field">
                                    @*<span class="holder" >FIRST NAME<span class="small">*</span></span>*@
                                    @*<input class="form-control" type="text" />*@
                                    @Html.TextBoxFor(m => m.FirstName, new { @required = "required", @id = "FirstName", @name = "Venue Name", @class = "form-control", @placeholder = "FIRST NAME" })
                                    @Html.ValidationMessageFor(model => model.FirstName, "", new { @class = "text-danger" })
                                </div>

                            </div>

                            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 form-group">

                                <div class="cust-field">
                                    @*<span class="holder">SURNAME<span class="small">*</span></span>*@
                                    @Html.TextBoxFor(m => m.LastName, new { @id = "LastName", @class = "form-control", @required = "required", @placeholder = "SURNAME" })
                                    @Html.ValidationMessageFor(model => model.LastName, "", new { @class = "text-danger" })
                                </div>

                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 form-group">

                                <div class="cust-field">
                                    @*<span class="holder">EMAIL <span class="small">*</span></span>*@
                                    @Html.TextBoxFor(m => m.UserName, new { @id = "UserName", @class = "form-control", @required = "required", @placeholder = "EMAIL" })
                                    @Html.ValidationMessageFor(model => model.UserName, "", new { @class = "text-danger" })
                                </div>

                            </div>



                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-12 clearfix">
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 form-group">

                                <div class="cust-field">
                                    @*<span class="holder">Venue Name <span class="small">*</span></span>*@
                                    @Html.TextBoxFor(m => m.VenueName, new { @id = "VenueName", @class = "form-control", @placeholder = "Venue Name", @required = "required", @data_toggle = "tooltip", @title = "eg. The Carvery Steakhouse & Bar" })
                                    @Html.ValidationMessageFor(model => model.VenueName, "", new { @class = "text-danger" })
                                </div>

                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 form-group">

                                <div class="cust-field">
                                    @*<span class="holder">Venue Name <span class="small">*</span></span>*@
                                    @Html.TextBoxFor(m => m.RestaurantName, new { @id = "RestaurantName", @class = "form-control", @placeholder = "Venue Short Name", @required = "required", @data_toggle = "tooltip", @title = "For online reservation URL – eg. “Carvery” (mondofi.com/carvery/online) vs. “The Carvery Steakhouse & Bar”" })
                                    @Html.ValidationMessageFor(model => model.RestaurantName, "", new { @class = "text-danger" })
                                </div>

                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 form-group">

                                <div class="cust-field">
                                    @*<span class="holder"> VENUE PHONE # <span class="small">*</span></span>*@
                                    @Html.TextBoxFor(m => m.phone, new { @id = "phone", @class = "form-control", @onkeypress = "return IsNumeric(event);", @ondrop = "return false;", @required = "required", @placeholder = "VENUE PHONE # " })
                                    @Html.ValidationMessageFor(model => model.phone, "", new { @class = "text-danger" })
                                </div>

                            </div>


                        </div>
                    </div>

                    <div class="col-xs-12 col-sm-12 clearfix">
                        <div class="row">

                            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 form-group">

                                <div class="cust-field">
                                    @*<span class="holder">PASSWORD <span class="small">*</span></span>*@
                                    @Html.PasswordFor(m => m.Password, new { @id = "Password", @class = "form-control buttonClick", @required = "required", @placeholder = "PASSWORD", @data_toggle = "tooltip", @title = "Must be at least 7 characters and contain at least one capital letter, one number and one lowercase letter." })
                                    @Html.ValidationMessageFor(model => model.Password, "", new { @class = "text-danger" })

                                </div>

                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 form-group">

                                <div class="cust-field">
                                    @*<span class="holder">CONFIRM PASSWORD <span class="small">*</span></span>*@

                                    @Html.PasswordFor(m => m.ConfirmPassword, new { @placeholder = "CONFIRM PASSWORD", @id = "ConfirmPassword", @class = "form-control cbuttonClick ", @required = "required" })
                                    @Html.ValidationMessageFor(model => model.ConfirmPassword, "", new { @class = "text-danger" })
                                </div>

                            </div>
                        </div>
                    </div>

                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 form-group cust-field">
                        @*<textarea name="" id="input" class="form-control" rows="3" placeholder="Additional Comments" required="required"></textarea>*@

                        @Html.TextAreaFor(m => m.Notes, new { @id = "Notes", @placeholder = "Additional Comments", @rows = "3", @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.Notes, "", new { @class = "text-danger" })
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 form-group terms-cond ">
                        @Html.CheckBoxFor(m => m.TermAndCondition, new { @required = "required",  })

                        @*<input id="rembMe" type="checkbox">*@ <label for="rembMe"><a data-toggle="modal" data-target="#TermsConditions" style="color:white; cursor:pointer"> &nbsp;&nbsp;I Agree with the Terms & Conditions</a>  </label><br />
                        @Html.ValidationMessageFor(model => model.TermAndCondition, "", new { @class = "text-danger" })
                    </div>

                    <div class="form-group clearfix">
                        <div class="col-xs-12  col-lg-6">
                            <div id="example2"></div>
                            <div id="captcha">

                            </div>
                            @*<img src="~/Content/MondofiThemeNew/images/captcha.png" />*@
                        </div>
                        <div class="col-xs-12 col-lg-6 text-right forgotPw">
                            @*<button onclick="submitForm()" type="button" class="btn btn-orange btnLogin btnreg text-uppercase">SEND</button>*@
                            <button id="sub" type="button" onclick="submitForm()" class="btn btn-orange btnLogin btnreg text-uppercase">SEND</button>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>

}
