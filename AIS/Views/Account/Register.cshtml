<script type="text/javascript">
    $(function () {
        $(".buttonClick").blur(function () {
            //alert("ok");
            $(".buttonClick").each(function () {

                var validated = true;
                if (this.value.length < 7)
                    validated = false;
                //if (!/\d/.test(this.value))
                //    validated = false;
                if (!/[a-z]/.test(this.value))
                    validated = false;
                if (!/[A-Z]/.test(this.value))
                    validated = false;
                if (!/[0-9]/.test(this.value))
                    validated = false;
                $("#passwordcheck").html(validated ? "<i class='halflings-icon ok green'></i>" :

                    alert("Must be at least 7 characters and contain at least one capital letter, one number and one lowercase letter.")

                     );
            });
        });
    });
</script>


