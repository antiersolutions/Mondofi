@{
    Layout = "~/Views/Shared/_LayoutMondofiNew.cshtml";
    ViewBag.Title = "Register";

    var price = string.Empty;

    try
    {
        price = Request["PL"].ToString();
    }
    catch (Exception)
    {
        price = string.Empty;

    }


}

