                @foreach (var day in Model.Week1)
                {


                    if (day != null)
                    {
                        var RCount = day.Reservations.Count;
                        var i = 1;

                        <td id="resPop1@(day.day.Day)" @Html.Raw(day.day == ViewBag.date ? "style=\"background-color:#e8867c;color:#fff;\"" : null) class="@(RCount > 0 ? (m <= 3 ? "reser reser-tool-tip" : "reser reser-tool-tip-l") : string.Empty)">
                            <span>@day.day.Day</span>
                            @if (RCount > 0)
                            {

                                <span>

                                </span>
                                <a href="javascript:void(0)" class="jt" rel="#resPop@(day.day.Day)" data-placement="@(m <= 3 ? "right" : "left")">
                                    <p @Html.Raw(day.day == ViewBag.date ? "style=\"color:#fff;\"" : null)>
                                        <span>@RCount</span> Reservations
                                    </p>
                                </a>
