@model AISModels.Designations
<div class="section-r-info">
    <div class="section-l-img">
        Status
    </div>
    <div class="section-r-info">
        <span class="c-status">
            @if (Model.IsAssignable == false)
            {
                <img src="/images/none-icon.png" alt="" />
                @Html.Raw("Unavailable for Section/Table Assignment")
            }
            else
            {
                <img src="/images/available-icon.png" alt="" />
                @Html.Raw("Available for Section/Table Assignment")
            }
        </span>
        <h1>@Model.Desgination</h1>
    </div>
</div>
