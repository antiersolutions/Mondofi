@model IEnumerable<AIS.Models.AlphabeticalMapping<AISModels.Designations>>
<div class="main-right-sec-in">
    @foreach (var oitem in Model)
    {
        <div class="section-head">
            <p>
                @oitem.FirstLetter
            </p>
        </div>
        foreach (var item in oitem.Items)
        {
            <ul class="people-status">
                <li id="@item.DesignationId">
                    @if (item.IsAssignable == false)
                    {
                        <img src="/images/none-icon.png" alt="" />

                    }
                    else
                    {
                        <img src="/images/available-icon.png" alt="" />

                    }
                    <h1 class="allergyshow">@item.Desgination</h1>
                </li>
            </ul>
        }
    }
</div>
