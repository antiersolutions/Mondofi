@model IEnumerable<AIS.Models.UserProfile>
<div class="hdStatus">
    <ul>
        <li>Id</li>
        <li style="width: 43px;">Color</li>
        <li style="width: 120px;">Staff Name @*<img src="/images/sort-icon.png" width="7" height="5">*@ </li>
        <li>Assign</li>
    </ul>
</div>
<div class="resInfo content_1">
    <ul class="reslistNew">
        @foreach (var staff in Model)
        {
            <li id="staffL@(staff.Id)">
                @Html.Action("GetStaffListItem", new { id = staff.Id })
            </li>
        }
    </ul>
</div>