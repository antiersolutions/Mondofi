@model AIS.Models.WaitingVM
<div id="waitNote">
    <pre>@Model.Notes</pre>
</div>
<div id="waitNoteEdt" style="display: none;">
    <textarea cols="36" rows="1" onkeyup="SaveWaitingNoteChanges('@(ViewBag.Parent)', '@(ViewBag.Target)')"
              onblur="CancelWaitingNoteChanges('@(ViewBag.Parent)')"></textarea>
</div>
