	display: block;
	padding: 16px 0;
}
.settings .menu-bar .left-menu ul li a:hover {
	color: #000000;
	background: url("../images/m-arrow.png") no-repeat scroll 95% 50% #f2f3f4;

}
.settings .menu-bar .left-menu ul li a.active {
	color: #000000;
	background: url("../images/m-arrow.png") no-repeat scroll 95% 50% #f2f3f4;
}

.settings .menu-bar .m-left-btn {
	position: absolute;
