* {
	margin: 0;
	padding: 0;
}
div {
	transition:all 0.3s ease-Out;
	-webkit-transition:all 0.3s ease-Out;
	-moz-transition:all 0.3s ease-Out;
	-o-transition:all 0.3s ease-Out;
}
body {
	background: #333333;
}
.wrapper {
	max-width: 1024px;
	min-height: 768px;
	margin: 0 auto;
	background: #fff;
	/*background: url(../images/body-bg.png);*/
	overflow: hidden;
}

.settings .menu-bar {
	width: 15%;
	float: left;
	min-height: 768px;
	position: relative;
	background: #fff;
	z-index: 9999;
	box-shadow: 0 0 3px #c7c7c7;
	-webkit-box-shadow: 0 0 3px #c7c7c7;
	-moz-box-shadow: 0 0 3px #c7c7c7;
	-o-box-shadow: 0 0 3px #c7c7c7;
}
.menu-bar-toggle {
	margin-left: -135px;
}
.settings .menu-bar .logo {
	width: 100%;
