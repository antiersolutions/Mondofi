	padding: 12px 0 8px;
	border-bottom: double 3px #eaeaea; 
}
.settings .menu-bar .left-menu {
	width: 100%;
	float: left;
}
.settings .menu-bar .left-menu ul {
	width: 100%;
	float: left;
